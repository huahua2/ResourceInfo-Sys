<% include header.ejs %>
<% include top_nav.ejs %>
<body style="padding-top:30px">
<div class="container-fluid">
    <div class="row">
        <% include left.ejs %>
        <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
            <h2 class="sub-header"><%= title %></h2> &nbsp;<a class="btn-link" href="/">返回</a>

            <div style="margin-top:30px;">
                <style>
                    dd, dt {
                        padding: 10px 0;
                    }

                    dd {
                        max-width: 500px
                    }
                </style>
                <dl class="dl-horizontal">

                    <dt>智库类别</dt>
                    <dd> <%= expert.Category %></dd>

                    <dt>类别细分项</dt>
                    <dd> <%= expert.Sub_Category %></dd>

                    <dt>单位</dt>
                    <dd> <%= expert.Company %></dd>
                    <dt>姓名</dt>
                    <dd>

                        <div style="display:<%=(expert.HeadUrl!="") ? "" : "none"%>">


                        <img   class="img-thumbnail" alt="头像"
                              src="<%= (expert.HeadUrl!="") ? ("/images/"+expert.HeadUrl) : ("data:image/svg+xml;base64,") %>" data-holder-rendered="true" style="width: 140px; height: 140px;">

                        <br/> <br/>
                        </div>
                        <span> <%= expert.UserName %></span>
                    </dd>



                    <dt>职务</dt>
                    <dd> <%= expert.Post %></dd>

                    <dt>职称</dt>
                    <dd> <%= expert.Title %></dd>

                    <dt>手机</dt>
                    <dd> <%= expert.Mobile %></dd>

                    <dt>办公电话</dt>
                    <dd> <%= expert.Office_Phone %></dd>

                    <dt>邮箱</dt>
                    <dd> <%= expert.Email %></dd>
                    <dt>微信、QQ</dt>
                    <dd> <%= expert.Social_Number %></dd>
                    <dt>常驻办公地点</dt>
                    <dd> <%= expert.Office_Add %></dd>

                    <dt>既往业务联系情况</dt>
                    <dd> <%= expert.Business_Contacts %></dd>

                    <dt>主要业绩/荣誉阐述</dt>
                    <dd> <%= expert.Main_Performance %></dd>

                    <dt>对接联系人</dt>
                    <dd> <%= expert.Docking_Contact %></dd>

                    <dt>个人简介</dt>
                    <dd> <%= expert.Profile %></dd>

                    <dt>备注</dt>
                    <dd> <%= expert.Remarks %></dd>



                </dl>
            </div>
        </div>
    </div>
</div>
<% include commonScript.ejs %>
</body><% include footer.ejs %>